{
    "name": "heroku-starter-project",
    "scripts": {
        "start": "cd ./src/client && npm start",
        "server": "nodemon --watch ./src/server ./src/server/server.ts",
        "deploy": "cd ./src/client && npm run build && cd ../.. && gulp deploy",
        "heroku": "cd ./deploy && git add . && git commit -m 'e' && git push heroku master",
        "lint": "eslint ./src --ext .ts",
        "lint-fix": "eslint ./src --ext .ts --fix",
        "jest": "jest"
    },
    "dependencies": {
        "body-parser": "^1.20.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "heroku": "^7.67.1",
        "mysql": "^2.18.1",
        "react-scripts": "^5.0.1",
        "socket.io": "^4.5.4",
        "sqlite3": "^5.1.2",
        "websocket": "^1.0.34"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jest": "^28.1.8",
        "@types/mysql": "^2.15.21",
        "@types/node": "^18.11.13",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "@typescript-eslint/eslint-plugin": "^5.30.5",
        "@typescript-eslint/parser": "^5.30.5",
        "eslint": "^8.19.0",
        "gulp": "^4.0.2",
        "gulp-clean": "^0.4.0",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^5.1.0",
        "jest": "^28.1.2",
        "nodemon": "^2.0.20",
        "sass": "^1.53.0",
        "source-map-loader": "^4.0.0",
        "ts-jest": "^28.0.5",
        "ts-node": "^10.8.2",
        "typescript": "^4.9.4",
        "webpack-stream": "^7.0.0"
    },
    "proxy": "http://localhost:4040"
}
